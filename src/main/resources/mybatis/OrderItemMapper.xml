<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.feicui.dao.OrderItemMapper">
	<insert id="saveOrderItem" parameterType="com.feicui.model.OrderItem">
		insert  into orderitem values(#{orderId},#{productId},#{buynum});
	</insert>
	<select id="findProductId" resultType="String">
		select product_id from orderitem where order_id = #{id}
	</select>
</mapper>